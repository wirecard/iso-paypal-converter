{
    "name": "wirecard/iso-paypal-converter",
    "description": "Converts ISO 3166-2 codes to the codes the PayPal API requires",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "library",
    "scripts": {
        "cs:check": "phpcs",
        "cs:fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test:coverage": "phpunit --colors=always --coverage-text"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Wirecard\\IsoToPayPal\\": "src/"
        }
    }
}
